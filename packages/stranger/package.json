{
  "name": "@everydayhero/stranger",
  "version": "2.1.0",
  "description": "A wrapper around existing CSS in JS solutions with as little complexity as possible",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "babel --out-dir dist --ignore __tests__ src",
    "prebuild": "rimraf dist",
    "prepublish": "npm run build",
    "size": "npm run build && bundle-size ./",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "css"
  ],
  "author": "Luke Brooker",
  "license": "MIT",
  "devDependencies": {
    "bundle-size": "^1.1.5",
    "enzyme": "^2.7.0",
    "jest-cli": "^16.0.1",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.5.4"
  },
  "peerDependencies": {
    "react": "^15.4.2"
  },
  "dependencies": {
    "cxs": "^3.0.0",
    "cxsync": "^2.2.0",
    "inline-style-prefixer": "^2.0.5",
    "@everydayhero/rug": "^2.1.0"
  },
  "standard": {
    "env": {
      "jest": true
    }
  }
}
